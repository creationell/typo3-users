<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <options>
            <ROOT>
                <TCEforms>
                    <sheetTitle>General settings</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>

                    <settings.pages>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>Pages to display on the dashboard</label>
                            <config>
                                <required>1</required>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>10</size>
                                <maxitems>999999</maxitems>
                                <minitems>1</minitems>
                                <show_thumbs>1</show_thumbs>
                            </config>
                        </TCEforms>
                    </settings.pages>


                </el>
            </ROOT>
        </options>

    </sheets>
</T3DataStructure>
